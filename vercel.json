{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/get-started.html",
      "destination": "/get-started",
      "permanent": true
    },
    {
      "source": "/dashboard_auth_gated.html",
      "destination": "/your-impact",
      "permanent": true
    },
    {
      "source": "/mission-control.html",
      "destination": "https://grateful.haylofriend.com",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/env.js",
      "destination": "/api/env.js"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/public/index.html"
    },
    {
      "source": "/get-started",
      "destination": "/public/get-started.html"
    },
    {
      "source": "/your-impact",
      "destination": "/public/dashboard_auth_gated.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/dashboard_auth_gated.html"
    },
    {
      "source": "/:path*",
      "destination": "/public/index.html"
    }
  ]
}
