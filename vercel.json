{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/env.js",
      "destination": "/api/env.js"
    }
  ],
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/get-started",
      "destination": "/login?redirect=/your-impact",
      "permanent": true
    },
    {
      "source": "/get-started.html",
      "destination": "/login?redirect=/your-impact",
      "permanent": true
    },
    {
      "source": "/create",
      "destination": "/login?redirect=/your-impact",
      "permanent": true
    },
    {
      "source": "/auth/google",
      "destination": "/login",
      "permanent": false
    },
    {
      "source": "/dashboard",
      "destination": "/your-impact",
      "permanent": true
    },
    {
      "source": "/mission-control.html",
      "destination": "https://grateful.haylofriend.com",
      "permanent": true
    }
  ]
}
